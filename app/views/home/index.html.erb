<%if current_user%>
<h1>
Welcome </h1>
</br>

<%#= link_to "Edit", edit_user_registration_path(current_user) %>
<%else%>
<div class="jumbotron">
  <% if user_signed_in? %>
    
    <%= link_to "Sign Out", destroy_user_session_path, :method => :delete, :class => 'btn  btn-danger' %>
  <% else %>
    <h1>Welcome, new user!</h1>
    <p class="lead">
      You must sign in or sign up before you can access our super sweet,
      top secret content.
    </p>
    <div class="row-fluid">
      <div class="span6">
        <%= link_to "Sign In", new_user_session_path, :class => "btn btn-success" %>
      </div>
      <div class="span6">
        <%= link_to "Sign Up", new_user_registration_path, :class => "btn btn-success" %>
      </div>
    </div>
  <% end %>
</div>
<%end%>