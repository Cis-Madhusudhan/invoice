<div class="span4">
<h2>Sign up</h2>

<%= form_for(resource, :as => resource_name, :url => registration_path(resource_name)) do |f| %>
  <%= devise_error_messages! %>

   <div><%= f.label :contact_name %><br />
  <%= f.text_field :contact_name %></div>

   <div><%= f.label :company %><br />
  <%= f.text_field :company %></div>

  <div><%= f.label :email %><br />
  <%= f.email_field :email %></div>
</div>
<div class="span4">
  </br></br></br>
   <div><%= f.label :username %><br />
  <%= f.text_field :username %></div>

  <div><%= f.label :password %><br />
  <%= f.password_field :password %></div>

  <div><%= f.label :password_confirmation %><br />
  <%= f.password_field :password_confirmation %></div>

  <div><%= f.check_box :terms_and_conditions %> <%= f.label :terms_and_conditions %></div>

  <div><%= f.submit "Sign up" %></div>
</div>
<% end %>
<div class="span4">
</br></br></br>
<%= render :partial => "devise/shared/links" %></div>
